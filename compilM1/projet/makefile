CC = gcc
CFLAGS = -g3 -Wall -c -fmessage-length=0
EXEC_NAME = main
INCLUDES = 
LIBS = 
OBJ_FILES = main.o tableSymboles.o

all : $(EXEC_NAME)

clean :
	rm $(EXEC_NAME) $(OBJ_FILES)

$(EXEC_NAME) : $(OBJ_FILES)
	$(CC) -o $(EXEC_NAME) $(OBJ_FILES) $(LIBS)

%.o: %.c
	gcc $(CFLAGS) $(INCLUDES) -o $@ -c $<