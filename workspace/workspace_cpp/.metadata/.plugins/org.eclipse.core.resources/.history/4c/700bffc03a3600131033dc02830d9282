#include <string>
using namespace std;

class Person {

private:

	string name;
	int age;

public:
	Person(){
		name = "toto";
		age = 25;
	};

	Person(string n, int a){
		name = n;
		age = a;
	};

	string getName(){return name;}
	int getAge(){return age;}

	void Person::dis(ostream & out){
		out << name << "a " << age;
	};
};



ostream & operator<< (ofstream &o,  Person& p  ){

	return p.displayP(&o);
};

