#include <string>
using namespace std;

class Person {

private:

	string name;
	int age;

public:
	Person(){
		name = "toto";
		age = 25;
	};

	Person(string n, int a){
		name = n;
		age = a;
	};

	string getName(){return name;}
	int getAge(){return age;}
};


ostream & operator<< (ofstream &o,  Person& p  ){
	cout << "La personne a";
	return o;
}

