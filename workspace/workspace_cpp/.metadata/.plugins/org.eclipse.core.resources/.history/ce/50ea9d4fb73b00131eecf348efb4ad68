using namespace std;
template<class T>
class Node{
protected:
	Node<T> *parent;
	Vector<Node<T>*> offspring;
	T value;
public:
	Node();
	Node(T,v);
	void add(Node<T> *node);
	void print(ostream& out);
	Node<T> *clone();
	friend ostream& operator <<(ostream& out, Node<T>&Node);
};



