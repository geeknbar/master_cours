package tp16;

public class Fabrication extends Thread{
	
	public int tires;
	public int motors;
	public int bodies;
	public int cars;
	
	
	public Fabrication(){
		tires = 0;
		motors = 0;
		bodies = 0;
		cars = 0;
	}

	public static void main(String[] args) {
		Fabrication f = new Fabrication();
		Tire t = new Tire();
		Body b = new Body();
		Motor m = new Motor();
	}
	
	
}
