	<wsdl:types>
		<xsd:schema targetNamespace="http://tempuri.org/AreaService/"
			xmlns:xsd="http://www.w3.org/2001/XMLSchema">
			<xsd:element name="area" type="xsd:float" />
			<xsd:element name="parameters" type="tns:dimensions" />
